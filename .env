# Postgres
POSTGRES_USER=app
POSTGRES_PASSWORD=app
POSTGRES_DB=indexer
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
DATABASE_URL=postgres://app:app@127.0.0.1:5432/indexer?sslmode=disable

# LocalStack (SQS)
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_DEFAULT_REGION=ap-northeast-2
LOCALSTACK_EDGE_PORT=4566
LOCALSTACK_URL=http://localstack:4566 # Docker internal value
SQS_QUEUE_NAME=gn-token-events

# Compose
COMPOSE_PROJECT_NAME=gnindexer
NETWORK_NAME=app-net

# GraphQL URL
GRAPHQL_ENDPOINT=https://indexer.onbloc.xyz/graphql/query
GRAPHQL_WS_ENDPOINT=wss://indexer.onbloc.xyz/graphql/query